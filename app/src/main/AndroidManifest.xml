<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ball.andy.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ball"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">

        </activity>
        <activity android:name=".ChoosePlayerActivity"></activity>
        <activity android:name=".GameActivity"></activity>

        <activity android:name=".MenuActivty">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>

        <activity android:name=".ScoreActivity"></activity>
        <activity android:name=".ModifyScoreActivity"></activity>

        <activity android:name=".GamingActivity"></activity>
    </application>

</manifest>
